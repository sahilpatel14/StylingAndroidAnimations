<?xml version="1.0" encoding="utf-8"?>

<!--
    todo 1
    The actual shape that we are going to morph. Take note that
    the shape is relatively simple. The pathData does not have much to show.
    Also, the resulting path data should have equal number of values as the
    initial path data.
-->

<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:viewportWidth="500"
    android:viewportHeight="500"
    android:width="500px"
    android:height="500px">

    <path android:name="square"
        android:fillColor="@color/colorAccent"
        android:pathData="M100,100 L400,100 L400,400 L100,400 z" />

</vector>